package;

class ScriptedClass extends ScriptedClassTest
{
	@:constructor(pre)
	public function new(arg1:String)
	{
		trace('Constructor.pre: $arg1');
	}

	@:constructor(post)
	public function new(arg1:String)
	{
		trace('Constructor.post: $arg1');
	}

	public function nnew(arg1:String)
		{
		trace('Constructor.pre: $arg1');
		super('test');
		if('hello' == 'hello')
		{
			trace('Constructor.post: $arg1');
		}
	}

	override public function info()
	{
		return 'I Scripted!!!!, super info: ${super.info()}';
	}

	public function argFunction(a1:Bool, a2:String, a3:String)
	{
		return '
		argument1: $a1, 
		argument2: $a2, 
		argument3: $a3';
	}

	public function string(arg:T)
	{
		return 'Script:' + arg.v;
	}

	public function stringArray(arg:Array<T>){
		return 'Script: stringArray -' + Std.string(arg);
	}
}